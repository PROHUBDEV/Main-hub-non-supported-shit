elseif game.PlaceId == 443406476 then
    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
    local Window = Library.CreateLib("FallenWare-PROJECT LAZARUS", "Midnight")

    -- MAIN
    local Main = Window:NewTab("Main")
    local MainSection = Main:NewSection("PROHUB V1 Project Lazarus")
    local Player = Window:NewTab("Player")
    local Player = Player:NewSection("Only works in voting area/Lobby only")
